        "ginkgo/yii2-amap": "^1.1",
        "mervick/yii2-emojionearea": "*",
        "kartik-v/yii2-editable": "^1.7",
        "kartik-v/yii2-datecontrol": "^1.9",
        "gietos/yii2-ionicons": "^2.0",
        "driftyco/ionicons": "^2.0",
        "moment/moment": "^2.24",
