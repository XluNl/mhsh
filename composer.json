        "yiizh/yii2-echarts": "^0.1.0",
        "kartik-v/yii2-tabs-x": "^1.2",
        "datatables/datatables": "^1.10",
        "fedemotta/yii2-widget-datatables": "^1.3",
        "nullref/yii2-datatables": "^1.1",
        "yiichina/yii2-icheck": "^0.5.0",
